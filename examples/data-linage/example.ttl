<https://y.com/data-product/company-sales/port/2025-balance-sheet> a dcat:DataService ;
  rdfs:label               "Balance Sheet" ;
  dcat:endpointURL         "https://y.com/data-product/company-sales/port/2025-c" ;
  dprod:isAccessServiceOf  <https://y.com/data-product/company-sales/port/2025-balance-sheet-dist> .

<https://y.com/data-product/company-sales/port/2025-balance-sheet-dist> a dcat:Distribution ;
  dcterms:format          "https://www.iana.org/assignments/media-types/application/json" ;
  dprod:isDistributionOf  <https://y.com/data-product/company-sales/dataset/2025-balance-sheet> .

<https://y.com/data-product/company-sales> a dprod:DataProduct ;
  dprod:outputPort  <https://y.com/data-product/company-sales/port/2025-sales> .

<https://y.com/data-product/company-hr/port/2025-payroll> a dcat:DataService .

<https://y.com/data-product/company-sales/dataset/2025-payroll> a dcat:Dataset ;
  rdfs:label          "Payroll" ;
  dcterms:conformsTo  "https://y.com/schema/Payroll" .

