<https://y.com/derived-quality-measurementA> a rdf:QualityMeasurement ;
  #TODO: to be defined in dprod? not in rdf:
  dprod:computedOn       y-prod:uk-bonds ;
  #TODO: to be defined in dprod? not in rdf:
  dprod:isMeasurementOf <https://y.com/derived-quality-measurementA-metric> ;
  rdf:value            1 .

