y-prod:uk-bonds a dprod:DataProduct ;
  dprod:outputPort y-prod:uk-bonds-port . 
  
y-prod:uk-bonds-port a dcat:DataService ;
  dcat:endpointURL         "https://y.com/uk-bonds/quality-report" ;
  dprod:isAccessServiceOf y-prod:uk-bonds-port-dist . 
  
y-prod:uk-bonds-port-dist a dcat:Distribution ;
  dprod:isDistributionOf y-prod:uk-bonds-port-dist-ds . 
  
y-prod:uk-bonds-port-dist-ds a dcat:Dataset ;
  dcterms:conformsTo  "https://www.w3.org/TR/vocab-dqv/#dqv:QualityMeasurement" .

<https://y.com/products/uk-bonds/yearlyPrices> a dcat:Dataset .
